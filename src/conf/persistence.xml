<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="DEFAULT_PU" transaction-type="JTA">
    <jta-data-source>jdbc/sample</jta-data-source>
    <class>modelo.Endereco</class>
    <class>modelo.Contato</class>
    <class>modelo.Frota</class>
    <class>modelo.Manutencao</class>
    <class>modelo.PessoaFisica</class>
    <class>modelo.Ocorrencia</class>
    <class>modelo.Locacao</class>
    <class>modelo.Entity_7</class>
    <class>modelo.Pedido</class>
    <class>modelo.Cliente</class>
    <class>modelo.Modelo</class>
    <class>modelo.Itinerario</class>
    <class>modelo.Estado</class>
    <class>modelo.CNH</class>
    <class>modelo.veiculo</class>
    <class>modelo.Cidade</class>
    <class>modelo.PessoaJuridica</class>
    <class>modelo.Acessorios</class>
    <class>modelo.Marca</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
